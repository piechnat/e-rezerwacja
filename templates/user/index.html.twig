{% extends 'base.html.twig' %}

{% block title %}{{ 'Lista użytkowników'|trans }}{% endblock %}

{% block main %}
  <h2>{{ 'Lista użytkowników'|trans }}</h2>
  <ul>
  {% for user in users %}
    <li>
      <a href="{{ path('user_show', {'id': user.id}) }}">{{ user.fullname }}</a>
    </li>
  {% endfor %}
  </ul>
{% endblock %}