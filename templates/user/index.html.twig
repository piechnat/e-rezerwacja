{% extends 'base.html.twig' %}
{% set main_title = 'Lista użytkowników' %}
{% block main %}

  <h2 class="main-title">{{ main_title|trans }}</h2>
  <ul>
  {% for user in users %}
    <li>
      <a href="{{ path('user_show', {'id': user.id}) }}">{{ user.fullname }}</a>
    </li>
  {% endfor %}
  </ul>
  
{% endblock %}