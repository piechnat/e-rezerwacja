{% extends 'reservation/view/base.html.twig' %}

{% block title %}Rezerwacje w dniu{% endblock %}

{% block table_view_form %}

  {{ form_start(form) }}
    <div class="form-wrapper">
      <div style="flex-basis: 100%">
        {{ form_row(form.tags) }}
      </div>
      {{ form_row(form.operation) }}
      <div class="form-group">
        {{ form_label(form.date) }}
        {{ form_errors(form.date) }}
        {{ form_widget(form.date) }}
        <button type="submit">
          <i class="fas fa-redo"></i>OK
        </button>
      </div>
    </div>
  {{ form_end(form) }}
  {{ include('jqslct2-load.html.twig') }}

{% endblock %}

{% block table_view_headers %}

  {% for header in table_view.headers %}
    <th>
      {{ header.title }}
    </th>
  {% endfor %}

{% endblock %}