{% extends 'base.html.twig' %}
{% set main_title = 'Lista rezerwacji' %}
{% block main %}

  <h2 class="main-title">{{ main_title|trans }}</h2>
    <ul>
    {% for item in item_list %}
      <li>
        <a href="{{ path('reservation_show', {id: item.id}) }}">
          {{ item.requester.fullname }}:
          {{ item.room.title }} -
          {{ item.beginTime|date('Y-m-d, H:i') }}-{{ item.endTime|date('H:i') }}
        </a>
      </li>
    {% endfor %}
    </ul>
    
{% endblock %}